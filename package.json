    "babel-register": "^6.26.0",
    "blipp": "^3.1.1",
    "boost": "^0.63.1",
    "chai": "^4.2.0",
    "dotenv": "^6.2.0",
    "glob": "^7.1.3",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
